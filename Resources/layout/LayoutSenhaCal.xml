<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:local="http://schemas.android.com/apk/res-auto"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<include
		android:id="@+id/toolbar"
		layout="@layout/toolbar"
		android:layout_height="25.5dp"
		android:background="@android:color/background_light"
		android:layout_marginBottom="12.5dp" />
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/main_content"
        android:layout_below="@id/toolbar"
        android:minWidth="25px"
        android:minHeight="25px"
        android:background="#ffeeeeee"
        android:contextClickable="false">
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/frameLayout1">
            <ImageView
                android:src="@mipmap/logosat"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/imageView1"
                android:background="@android:color/background_light"/>
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_marginTop="3dp"
            android:layout_width="match_parent"
            android:layout_height="50.0dp"
            android:id="@+id/frameLayout9">
            <DigitalClock
                android:layout_width="match_parent"
                android:id="@+id/digitalClock2"
                android:gravity="center"
                android:textSize="30dp"
                android:layout_height="47.0dp"
                android:background="#ffddceb7" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="@android:color/background_dark"
            android:layout_width="match_parent"
            android:layout_height="2.0dp"
            android:id="@+id/frameLayout14" />

        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="@android:color/background_dark"
            android:layout_width="match_parent"
            android:layout_height="2.0dp"
            android:id="@+id/frameLayout14" />
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="60.5dp"
            android:id="@+id/frameLayout10">
            <TextView
                android:text="SENHA"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/textView1"
                android:textStyle="bold|normal"
                android:textSize="30dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="450dp"
                android:gravity="center|left" />
            <EditText
                android:text=""
                android:maxLength="24"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/TxtSenha"
                android:gravity="center|left"
                android:layout_marginLeft="250dp"
                android:inputType="none|textCapCharacters|numberPassword"
                android:layout_marginRight="120dp"
                android:textAllCaps="true"
                android:textStyle="bold|normal"
                android:textSize="30dp" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="@android:color/background_dark"
            android:layout_width="match_parent"
            android:layout_height="2.0dp"
            android:id="@+id/frameLayout14" />
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="88.5dp"
            android:id="@+id/frameLayout17"
            android:layout_marginTop="610dp">
            <ImageButton
                android:src="@drawable/save"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/BtnSalvar"
                android:layout_marginLeft="60dp"
                android:layout_marginRight="400dp"
                android:background="@android:color/background_light"
                android:layout_marginTop="5dp" />
            <ImageButton
                android:src="@drawable/voltar"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/BtnSair"
                android:layout_marginLeft="400dp"
                android:layout_marginRight="60dp"
                android:background="@android:color/background_light"
                android:layout_marginTop="5dp" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="@android:color/background_dark"
            android:layout_width="match_parent"
            android:layout_height="2.0dp"
            android:id="@+id/frameLayout14" />

    </LinearLayout>
</RelativeLayout>