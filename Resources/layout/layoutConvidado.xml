<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:local="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <include
		android:id="@+id/toolbar"
		layout="@layout/toolbar"
		android:background="@android:color/background_light" />
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/main_content"
        android:layout_below="@id/toolbar"
        android:minWidth="25px"
        android:minHeight="25px"
        android:background="#ffeeeeee"
        android:contextClickable="false">
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:id="@+id/frameLayout1">
            <ImageView
                android:src="@drawable/logowl"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/imageView1"
                android:background="@android:color/background_light"/>
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_marginTop="3dp"
            android:layout_width="match_parent"
            android:layout_height="50.0dp"
            android:id="@+id/frameLayout9">
            <DigitalClock
                android:layout_width="match_parent"
                android:id="@+id/digitalClock2"
                android:textSize="30sp"
                android:gravity="center"
                android:layout_height="47.0dp"
                android:background="#ffddceb7"
                android:textStyle="normal|bold" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="@android:color/background_dark"
            android:layout_width="match_parent"
            android:layout_height="2.0dp"
            android:id="@+id/frameLayout14" />
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="50.5dp"
            android:id="@+id/frameLayout19">
            <TextView
                android:text="CÃ“DIGO"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/textView111"
                android:textStyle="bold|normal"
                android:textSize="20dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="300dp"
                android:gravity="center|left" />
            <TextView
                android:text=""
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/TxtCodigoColaborador"
                android:gravity="center|left"
                android:layout_marginLeft="100dp"
                android:inputType="textCapCharacters"
                android:layout_marginRight="20dp" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="50.5dp"
            android:id="@+id/frameLayout1411">
            <TextView
                android:text="COLABORADOR"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/textView2"
                android:gravity="center|left"
                android:layout_marginLeft="10dp"
                android:textStyle="normal|bold"
                android:layout_marginRight="250dp"
                android:textSize="20dp" />
            <Spinner
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/spinnercolaborador"
                android:layout_marginLeft="160dp"
                android:layout_marginRight="10dp" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="50.5dp"
            android:id="@+id/frameLayout11"
            android:layout_marginTop="5dp">
            <TextView
                android:text="NOME"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/textView1"
                android:layout_marginLeft="13.9dp"
                android:gravity="center|left"
                android:textStyle="normal|bold"
                android:textSize="20dp"
                android:textColor="@android:color/background_dark" />
            <EditText
                android:text=""
                android:inputType="text"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/TxtNomeConvidado"
                android:maxLength="232"
                android:textStyle="bold|normal"
                android:layout_marginLeft="100dp"
                android:textSize="20dp"
                android:layout_gravity="center"
                android:gravity="center|left"
                android:layout_marginRight="40dp"
                android:textColor="@android:color/black" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="50.5dp"
            android:id="@+id/frameLayout15">
            <TextView
                android:text="CONSOME BEBIDAS"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/textView2"
                android:gravity="center|left"
                android:layout_marginLeft="10dp"
                android:textStyle="normal|bold"
                android:layout_marginRight="150dp"
                android:textSize="20dp" />
            <Spinner
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/spinnerconsomeconvidado"
                android:layout_marginLeft="220dp"
                android:layout_marginRight="20dp" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="#ff000000"
            android:layout_width="match_parent"
            android:layout_height="2.0dp"
            android:id="@+id/frameLayout1410" />
        <RelativeLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="1.5dp"
            android:layout_marginTop="0dp"
            android:id="@+id/relativeLayout1"
            android:background="@android:color/background_dark" />
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="100.0dp"
            android:id="@+id/frameLayout7"
            android:background="#ffddceb7"
            android:layout_marginTop="3dp"
            android:defaultFocusHighlightEnabled="false">
            <ImageButton
                android:src="@drawable/save"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:id="@+id/BtnSalvarConvidado"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="280dp"
                android:layout_marginBottom="35dp"
                android:background="@android:color/background_light"
                android:hapticFeedbackEnabled="false" />

            <ImageButton
                android:src="@drawable/excluir"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:id="@+id/BtnExcluirConvidado"
                android:layout_marginLeft="150dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="150dp"
                android:layout_marginBottom="35dp"
                android:background="@android:color/background_light"
                android:hapticFeedbackEnabled="false" />

            <ImageButton
                android:src="@drawable/voltar"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_marginTop="5dp"
                android:id="@+id/BtnSairConvidado"
                android:layout_marginLeft="280dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="35dp"
                android:background="@android:color/background_light"
                android:foregroundTintMode="src_atop" />
        </FrameLayout>
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:background="#ff000000"
            android:layout_width="match_parent"
            android:layout_height="2.0dp"
            android:id="@+id/frameLayout146" />
        <TextView
            android:text="  CODIGO  CONS. NOME"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="match_parent"
            android:textSize="20dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView1"
            android:background="#ff000000"
            android:backgroundTint="@android:color/black"
            android:textColor="@android:color/background_light"
            android:textStyle="bold|normal"
            android:singleLine="false" />
        <FrameLayout
            android:minWidth="25px"
            android:minHeight="25px"
            android:layout_width="match_parent"
            android:layout_height="520.0dp"
            android:id="@+id/frameLayout18">
            <ListView
                android:minWidth="25px"
                android:minHeight="25px"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/ListaClientes" />
        </FrameLayout>
    </LinearLayout>
</RelativeLayout>

